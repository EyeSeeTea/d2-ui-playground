{
    "name": "d2-ui-playground",
    "version": "0.0.1",
    "description": "DHIS2 d2-ui playground of @dhis2/d2-ui-xyz packages",
    "main": "src/app.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dhis2/d2-ui.git"
    },
    "license": "BSD-3-Clause",
    "author": "Bjørn Sandvik",
    "scripts": {
        "prestart": "d2-manifest package.json manifest.webapp",
        "start": "webpack-dev-server",
        "build": "rm -rf build && NODE_ENV=production webpack --progress && npm run manifest && cp -r i18n build",
        "postbuild": "cp icon.png ./build/",
        "validate": "npm ls --depth 0 || yarn list",
        "manifest": "d2-manifest package.json build/manifest.webapp",
        "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
        "localize": "yarn extract-pot && d2-i18n-generate -n d2-ui-interpretations -p ./i18n/ -o ./src/locales/",
        "deploy": "yarn build && mvn clean deploy",
        "lint": "eslint ./src",
        "profile": "yarn start -- --profile",
        "prettify": "prettier \"src/{**/*,*}.js\" --write"
    },
    "dependencies": {
        "@dhis2/d2-i18n": "^1.0.3",
        "@dhis2/d2-i18n-extract": "^1.0.7",
        "@dhis2/d2-i18n-generate": "^1.0.19",
        "@dhis2/d2-ui-core": "^1.1.4",
        "@dhis2/d2-ui-sharing-dialog": "^1.0.9",
        "classnames": "^2.2.5",
        "d2": "^29.1.1",
        "husky": "^1.0.0-rc.8",
        "i18next": "^10.4.1",
        "i18next-xhr-backend": "^1.5.1",
        "lodash": "^4.17.5",
        "loglevel": "^1.6.1",
        "material-ui": "^0.19.3",
        "material-ui-icons": "^1.0.0-beta.36",
        "moment": "^2.22.2",
        "postcss-rtl": "^1.3.0",
        "prop-types": "^15.5.10",
        "query-string": "^6.1.0",
        "react": "^16.1.1",
        "react-dom": "^16.1.1",
        "react-tap-event-plugin": "^3.0.2"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.2",
        "babel-loader": "^7.1.2",
        "babel-plugin-istanbul": "^4.1.4",
        "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
        "babel-plugin-transform-object-assign": "^6.22.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "css-loader": "^0.28.7",
        "d2-manifest": "^1.0.0",
        "html-webpack-plugin": "^2.30.1",
        "node-sass": "^4.5.3",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.18.2",
        "webpack": "^3.11.0",
        "webpack-dev-server": "^2.11.1"
    },
    "manifest.webapp": {
        "name": "DHIS2 Maps",
        "icons": {
            "48": "icon.png"
        },
        "developer": {
            "url": "",
            "name": "Bjørn Sandvik"
        },
        "activities": {
            "dhis": {
                "href": ".."
            }
        }
    }
}
